server:
  port: 8787
spring:
  application:
    name: "Sample-blog-website"
  datasource:
    url: jdbc:postgresql://localhost:5432/BlogSSU 
    username: 
    password: 
    driver-class-name: org.postgresql.Driver
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true
  mail:
    host: smtp.gmail.com
    port: 587
    username: 
    password: 
    properties:
      mail:
        smtp:
          auth: true
          starttls:
            enable: true